<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
	<configuration>

		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>Grasshopper</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>GHCHIRPING</reportName>

		<!-- timestamp format -->
		<timeStampFormat>MMM dd, yyyy HH:mm:ss</timeStampFormat>

		<!-- settings to enable/disable views -->
		<enableCategoryView>true</enableCategoryView>
		<enableAuthorView>true</enableAuthorView>
		<enableExceptionView>true</enableExceptionView>
		<enableTestRunnerLogsView>true</enableTestRunnerLogsView>
		
		<!-- view report without internet -->
		<offlineMode>false</offlineMode>

		<!-- custom javascript -->
		<scripts>
			<![CDATA[
				<!-- $(document).ready(function() {
				    $('.test').click(function() { console.log('test'); });
				});-->
			]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[
			 <!-- .test.active { border: 1px solid #ccc; }-->
			]]>
		</styles>

	</configuration>
</extentreports>